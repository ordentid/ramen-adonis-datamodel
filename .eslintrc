{
    "env": {
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "rules": {
      "class-methods-use-this": "off",
      "object-curly-newline": ["error", { "multiline": true }],
      "global-require": "off",
      "arrow-parens": ["error", "as-needed"],
      "no-param-reassign": ["error", { "props": false }]
    },
    "globals": {
      "use": true
    }
  }